{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": false,
    "object-literal-sort-keys": false,
    "no-console": [],
    "no-trailing-whitespace": true,
    "no-unused-variable": false,
    "quotemark": [true, "single", "avoid-escape"],
    "trailing-comma": false,
    "comment-format": []
  }
}
